#kaocha/v1
{:tests    [{:id           :unit
             :ns-patterns  ["-test$" "-spec$"]
             :test-paths   ["src/test"]
             :skip-meta    [:integration]
             :source-paths ["src/main"]}
            {:id           :integration
             :ns-patterns  ["-test$" "-spec$"]
             :test-paths   ["src/test"]
             :focus-meta   [:integration]
             :skip-meta    [:unit]
             :source-paths ["src/main"]}]
 ;:reporter [kaocha.report/documentation]
 :reporter [fulcro-spec.reporters.terminal/fulcro-report]
 :plugins  []
 :bindings {kaocha.stacktrace/*stacktrace-filters*
            ["java.lang." "java.util.concurrent."
             "clojure.test$" "clojure.lang." "clojure.core" "clojure.main" "clojure.walk"
             "orchestra." "kaocha." "lambdaisland.tools.namespace.reload"]}}
